{
    "source": {
      "include": ["src/"],
      "includePattern": ".js",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        },
        "tutorials": "./examples",
        "destination": "./docs/"
    }
}