{
    "source": {
      "include": ["src/"],
      "includePattern": ".js",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "tutorials": "./examples",
        "template": "node_modules/prismarine-jsdoc-theme",
        "destination": "./docs/",
        "readme": "./README.md"
    },
    "docdash": {
        "search": true
    }
}